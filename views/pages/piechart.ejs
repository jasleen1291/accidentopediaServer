<!DOCTYPE html>
<html lang="en">
<script type="text/javascript" src="https://www.google.com/jsapi"></script>

<script type="text/javascript">
    google.load("visualization", "1", {packages:["corechart"]});
    google.setOnLoadCallback(drawChart);
    function drawChart() {
    var st=["#b87333","silver","gold","#e5e4e2"];
var dat=<%-aux%>;
var i=0;
dat[0].push( { role: 'style' });
for (i = 0; i < dat.length; i++) { 
if(i!==0)
   dat[i].push(st[i%5]);
}

      var data = google.visualization.arrayToDataTable(dat);

      var view = new google.visualization.DataView(data);
      view.setColumns([0, 1,
                       { calc: "stringify",
                         sourceColumn: 1,
                         type: "string",
                         role: "annotation" },
                       2]);

      var options = {
       fontSize:10, 
            legendFontSize:14, 
chartArea: {'width': '100%', 'height': '90%'},
        width: "100%",
        height: "100%",
        bar: {groupWidth: "95%"},
        legend: { position: "left" },
      };
      var chart = new google.visualization.PieChart(document.getElementById("barchart_values"));
      chart.draw(view, options);
  }
  </script>

<body class="container" style="{height:100}">

	

	
		<div id="barchart_values" style="width: 100%; height: 100%;position:absolute"></div>
	

	
	
</body>
</html>